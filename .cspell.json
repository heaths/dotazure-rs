{
  "version": "0.2",
  "language": "en",
  "useGitignore": true,
  "ignorePaths": [
    ".cspell.json",
    ".devcontainer/",
    ".github/",
    ".vscode/",
    "rust-toolchain.toml"
  ],
  "words": [
    "clippy",
    "dotazure",
    "repr",
    "rustlang",
    "rustup"
  ],
  "dictionaryDefinitions": [
    {
      "name": "crates",
      "words": [
        "dotenvy",
        "serde"
      ],
      "noSuggest": true
    }
  ],
  "overrides": [
    {
      "filename": "Cargo.toml",
      "dictionaries": [
        "crates"
      ]
    },
    {
      "filename": "**/*.md",
      "dictionaries": [
        "crates",
        "rust"
      ]
    },
    {
      "filename": "**/*.rs",
      "dictionaries": [
        "crates"
      ]
    }
  ]
}
